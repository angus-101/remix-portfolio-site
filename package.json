{
  "name": "remix-portfolio-site",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "run:dev": "sst bind remix dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc --noEmit",
    "sst:deploy": "sst deploy",
    "sst:deploy:dev": "sst deploy --stage dev",
    "sst:dev": "sst dev",
    "lint:js": "eslint --cache --cache-location ./node_modules/.cache/eslint app",
    "scss:build": "sass app/scss:styles",
    "scss:watch": "sass --watch app/scss:styles",
    "dev": "run-p scss:watch run:dev"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.4.0",
    "@remix-run/node": "^2.4.0",
    "@remix-run/react": "^2.4.0",
    "@remix-run/serve": "^2.4.0",
    "isbot": "^3.6.8",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "aws-cdk-lib": "2.110.1",
    "constructs": "10.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "sst": "^2.38.7",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
